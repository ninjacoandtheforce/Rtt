.summaryButton {
    position: fixed;
    opacity: 0;
    visibility: hidden;
    overflow: hidden;
    text-align: center;
    z-index: 3000;
    background-color: #3382D1;
    color: #eeeeee;
    width: 260px;
    height: 48px;
    line-height: 48px;
    right: 90px;
    bottom: 30px;
    padding-top: 2px;
    border-top-left-radius: 0px;
    border-top-right-radius: 0px;
    border-bottom-right-radius: 0px;
    border-bottom-left-radius: 0px;
    -webkit-transition: all 0.5s ease-in-out;
    -moz-transition: all 0.5s ease-in-out;
    -ms-transition: all 0.5s ease-in-out;
    -o-transition: all 0.5s ease-in-out;
    transition: all 0.5s ease-in-out;
    border: none;
}
.summaryButton:hover {
    background-color: #007FFF;
}
.summaryButton.show {
    visibility:visible;
    cursor:pointer;
    opacity: 1.0;
}
.summaryButton i.fa {
    line-height: inherit;
}

.quoteSummaryLabel {
    vertical-align: top;
